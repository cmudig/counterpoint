{
  "compilerOptions": {
    "target": "es2015"
  },
  "typedocOptions": {
    "entryPoints": ["lib"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "exclude": ["tests", "dist", "node_modules"],
    "readme": "README.md",
    "categorizeByGroup": false,
    "plugin": ["typedoc-theme-hierarchy"],
    "theme": "hierarchy"
  }
}
